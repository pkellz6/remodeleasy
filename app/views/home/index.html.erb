<h1>RemodelEasy</h1>
<div>
<h2 class = "signupmaster">
  <% if current_user.nil? %>
    <%= link_to 'Sign Up', new_user_registration_path %>
  <% else %>
    Signed In 
  <% end %>
</h2>
</div>
<div>
<h2 class = "loginmaster">
<% if current_user.nil? %>
  <%= link_to 'Log In', new_user_session_path %>
<% else %>
  <%= link_to 'Log Out', destroy_user_session_path, method: :delete %>
<% end %>
</h2>
</div>
